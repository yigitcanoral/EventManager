<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yigitcan.EventManager">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.yigitcan.EventManager.Splashactivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.yigitcan.EventManager.Activitysignup"/>
        <activity android:name="com.example.yigitcan.EventManager.Activityaddvenue" />
        <activity android:name="com.example.yigitcan.EventManager.Activitymainpage" />
        <activity android:name="com.example.yigitcan.EventManager.Activityshowvenues" />
        <activity android:name="com.example.yigitcan.EventManager.Activitylogin"></activity>
    </application>

</manifest>